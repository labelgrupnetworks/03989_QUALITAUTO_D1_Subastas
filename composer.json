{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "v2.0.0",
        "guzzlehttp/guzzle": "7.4.5",
        "intervention/image": "2.7.2",
        "laravel/framework": "v9.19.0",
        "laravel/sanctum": "v2.15.1",
        "laravel/socialite": "v5.5.5",
        "laravel/tinker": "v2.7.2",
        "maatwebsite/excel": "3.1.58",
        "neitanod/forceutf8": "v2.0.4",
        "opcodesio/log-viewer": "2.4",
        "psr/simple-cache": "1.0.0",
        "setasign/fpdf": "1.8.4",
        "setasign/fpdi": "v2.3.6",
        "simplesoftwareio/simple-qrcode": "4.2.0",
        "spatie/ssl-certificate": "2.6.5",
        "wisembly/elephant.io": "^3.3",
        "yajra/laravel-oci8": "v9.0.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "v3.7.0",
        "fakerphp/faker": "v1.19.0",
        "laravel/sail": "v1.15.0",
        "mockery/mockery": "1.5.0",
        "nunomaduro/collision": "v6.2.1",
        "phpunit/phpunit": "9.5.21",
        "spatie/laravel-ignition": "1.3.1"
    },
    "autoload": {
		"files": [
			"app/Http/Helpers/Helper.php"
		],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
			"Yajra\\Oci8\\Query\\Grammars\\": "app/packages/Yajra",
			"Yajra\\Pdo\\Oci8\\": "app/packages/Yajra"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
	},
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
		"post-install-cmd": [
			"@php artisan storage:link && echo 'Â¡Enlace de almacenamiento creado correctamente!'"
		],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
